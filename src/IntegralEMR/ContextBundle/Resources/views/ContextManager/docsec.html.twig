<h2>Document Sections</h2>
<table>
    {% for sec in sections %}
        <tr>
            <td>
                {{sec.getLongDesc()}}
            </td>
            <td>
                {{sec.getCode()}}
            </td>
            <td>
                {{sec.getCode_type()}}
            </td>
        </tr>
    {% endfor %}

</table>